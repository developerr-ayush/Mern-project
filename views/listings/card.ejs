<a href="/listing/<%=data.id%>" class="card h-100">
    <img src="<%= data.image.url %>" class="card-img-top" alt="<%=data.image.filename%>" />
    <div class="card-body">
        <h5 class="card-title">
            <%= data.title %>
        </h5>
        <p class="card-text">
            <%= data.description %>
        </p>
    </div>
</a>